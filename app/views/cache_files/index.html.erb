<p id="notice"><%= notice %></p>

<h1>Listing Cache Files</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Cache</th>
      <th>URL</th>
      <th>Status</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cache_files.each do |cache_file| %>
      <tr>
        <td><%= cache_file.name %></td>
        <td><%= cache_file.cache %></td>
        <td><%= media_file.url %></td>
        <td><%= media_file.status %></td>
        <td><%= link_to 'Show', cache_file %></td>
        <td><%= link_to 'Edit', edit_cache_file_path(cache_file) %></td>
        <td><%= link_to 'Destroy', cache_file, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cache file', new_cache_file_path %>
